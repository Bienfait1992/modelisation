// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model machines  {
id        Int     @id @default(autoincrement())
Tag       String   @unique
Modele    String
Fabriquant String
apprenant Apprenant[]
}

model Apprenant  {
id        Int     @id @default(autoincrement())
Matricule String @unique
Tag_machines   machines @relation(fields: [machinesId], references: [id])
machinesId Int
Code_cohorte Cohorte @relation(fields: [code_cohorteId], references: [id])
code_cohorteId Int
Prenom    String
Nom       String
Postnom   String
Date_de_naissance DateTime
Adresse   String
Email     String
Telephone Int
}

model Session  {
id          Int     @id @default(autoincrement())
Id_session String @unique
cohorteId      Int  
Annee       String
Type        String
Ville       String
chorte      Cohorte[]
}

model Cohorte  {
id          Int     @id @default(autoincrement())
Code_cohorte Int     @unique
Description String
code_session     Session   @relation(fields: [code_sessionId], references: [id])
code_sessionId Int   
apprenant   Apprenant[]
coaching Coaching []

}

model Coaches  {
id          Int     @id @default(autoincrement())
Matricule_coach     String    @unique
Prenom              String
Nom                 String
Postnom             String
Date_de_naissance  DateTime 
Adresse             String
Email               String
Telephone           String
coaching Coaching []

}

model Coaching  {
id          Int     @id @default(autoincrement())
Matricule_coach     Coaches @relation(fields: [coachesId], references: [id])  
coachesId             Int
Code_chorte         Cohorte   @relation(fields: [cohorteId], references: [id])  
cohorteId              Int

}

